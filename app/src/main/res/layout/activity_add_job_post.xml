<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AddJobPostActivity">

    <com.google.android.material.navigation.NavigationView
            android:layout_width="0dp"
            android:layout_height="55dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        <Button
                android:id="@+id/goBackFrmJob"
                android:layout_width="54dp"
                android:layout_height="match_parent"
                android:background="#00000000"
                android:backgroundTint="#FFFFFF"
                android:text="@string/back"
                app:icon="?attr/actionModeCloseDrawable"
                app:iconTint="#95000000"
                app:iconTintMode="src_in" />

    </com.google.android.material.navigation.NavigationView>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.35000002"
            android:layout_marginTop="75dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/background_purple"
                android:id="@+id/linearLayout2">

            <TextView
                    android:text="@string/add_job_post"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView26"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:textColor="#FFFFFF"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="558dp"
                    tools:layout_editor_absoluteY="1dp">

                <TextView
                        android:id="@+id/textView20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/position"
                        android:textColor="#020101"
                        android:textStyle="normal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.085"
                        app:layout_constraintVertical_bias="0.057"
                        android:layout_marginTop="-10dp" />

                <EditText
                        android:id="@+id/editTextTextMultiLine"
                        android:layout_width="318dp"
                        android:layout_height="53dp"
                        android:autofillHints=""
                        android:ems="10"
                        android:gravity="start|top"
                        android:inputType="textMultiLine"
                        tools:ignore="LabelFor,SpeakableTextPresentCheck"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.475"
                        app:layout_constraintVertical_bias="0.099"
                        android:textColor="#040202" />

                <TextView
                        android:id="@+id/textView21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/qualification"
                        android:textColor="#020101"
                        android:textStyle="normal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.102"
                        app:layout_constraintVertical_bias="0.225"
                        android:layout_marginTop="-10dp" />

                <EditText
                        android:id="@+id/editTextTextMultiLine3"
                        android:layout_width="316dp"
                        android:layout_height="45dp"
                        android:autofillHints=""
                        android:ems="10"
                        android:gravity="start|top"
                        android:inputType="textMultiLine"
                        android:textColor="#040202"
                        tools:ignore="LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="@+id/button5"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.492"
                        app:layout_constraintVertical_bias="0.274" />

                <TextView
                        android:id="@+id/textView22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/company_name"
                        android:textColor="#050404"
                        android:textStyle="normal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="@+id/button5"
                        app:layout_constraintHorizontal_bias="0.107"
                        app:layout_constraintVertical_bias="0.36" />

                <EditText
                        android:id="@+id/editTextTextMultiLine5"
                        android:layout_width="320dp"
                        android:layout_height="50dp"
                        android:autofillHints=""
                        android:ems="10"
                        android:gravity="start|top"
                        android:hint=""
                        android:inputType="textMultiLine"
                        tools:ignore="LabelFor"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="@+id/button5"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.542"
                        app:layout_constraintVertical_bias="0.443" />

                <TextView
                        android:id="@+id/textView23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/start_date"
                        android:textColor="#050404"
                        android:textStyle="normal"
                        tools:ignore="VisualLintOverlap"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.1"
                        app:layout_constraintVertical_bias="0.536" />

                <EditText
                        android:id="@+id/editTextDate"
                        android:layout_width="122dp"
                        android:layout_height="52dp"
                        android:autofillHints=""
                        android:ems="10"
                        android:inputType="date"
                        tools:ignore="LabelFor,SpeakableTextPresentCheck,VisualLintOverlap"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.116"
                        app:layout_constraintVertical_bias="0.632" />

                <TextView
                        android:id="@+id/textView24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/end_date"
                        android:textColor="#040202"
                        android:textStyle="normal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.709"
                        app:layout_constraintVertical_bias="0.536" />

                <EditText
                        android:id="@+id/editTextDate2"
                        android:layout_width="116dp"
                        android:layout_height="51dp"
                        android:autofillHints=""
                        android:ems="10"
                        android:inputType="date"
                        tools:ignore="LabelFor,SpeakableTextPresentCheck"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintVertical_bias="0.631"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.897" />

                <TextView
                        android:id="@+id/textView25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/job_description"
                        android:textColor="#050404"
                        android:textStyle="normal"
                        tools:ignore="VisualLintOverlap"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.099"
                        app:layout_constraintVertical_bias="0.716" />

                <EditText
                        android:id="@+id/editTextTextMultiLine2"
                        android:layout_width="324dp"
                        android:layout_height="55dp"
                        android:autofillHints=""
                        android:ems="10"
                        android:gravity="start|top"
                        android:hint=""
                        android:inputType="textMultiLine"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/editTextTextMultiLine"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintVertical_bias="0.812"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.49" />

                <Button
                        android:id="@+id/button5"
                        android:layout_width="279dp"
                        android:layout_height="65dp"
                        android:text="@string/save__"
                        tools:ignore="VisualLintOverlap"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintVertical_bias="0.985" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
